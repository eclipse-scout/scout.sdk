<xs:schema
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns="https://www.eclipse.org/schema/common"
    targetNamespace="https://www.eclipse.org/schema/common">

  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="https://download.eclipse.org/scout/static/resources/xml.xsd"/>

  <xs:simpleType name="CodeTypeType">
