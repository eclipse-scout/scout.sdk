<feature
      id="org.eclipse.scout.sdk-feature"
      label="%featureName"
      version="8.0.0.008_Photon_RC3"
      provider-name="%providerName"
      plugin="org.eclipse.scout.sdk.s2e"
      license-feature="org.eclipse.license"
      license-feature-version="0.0.0">

   <description url="http://www.eclipse.org/scout/">
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="http://download.eclipse.org/releases/oxygen"/>
      <discovery label="%updateSiteName" url="http://download.eclipse.org/releases/oxygen"/>
   </url>

   <requires>
      <import feature="org.eclipse.platform" version="4.6.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.jdt" version="3.12.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.m2e.feature" version="1.7.0" match="greaterOrEqual"/>
      <import plugin="org.apache.commons.lang3" version="3.1" match="greaterOrEqual"/> <!-- Is included in the p2 update site via category.xml -->
      <import plugin="javax.wsdl" version="1.6.2" match="greaterOrEqual"/> <!-- Is included in the p2 update site via category.xml -->
   </requires>

   <plugin
         id="org.eclipse.scout.sdk.s2e"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

   <plugin
         id="org.eclipse.scout.sdk.s2e.doc"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.scout.sdk.s2e.nls"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.scout.sdk.s2e.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
