<?xml version="1.0" encoding="UTF-8"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="@@BUNDLE_SWT_NAME@@.perspective.Perspective"
            fixed="false"
            icon="resources/icons/eclipse_scout.gif"
            id="@@BUNDLE_SWT_NAME@@.perspective.Perspective"
            name="@@ALIAS@@"/>
   </extension>
   <extension
            point="org.eclipse.ui.views">
         <view
               allowMultiple="true"
               category="@@BUNDLE_SWT_NAME@@.viewCategory"
               class="@@BUNDLE_SWT_NAME@@.views.CenterView"
               icon="resources/icons/eclipse_scout.gif"
               id="@@BUNDLE_SWT_NAME@@.views.CenterView"
               name="Center View"
               restorable="true">
         </view>
         <view
               allowMultiple="true"
               category="@@BUNDLE_SWT_NAME@@.viewCategory"
               class="@@BUNDLE_SWT_NAME@@.views.WestView"
               icon="resources/icons/eclipse_scout.gif"
               id="@@BUNDLE_SWT_NAME@@.views.WestView"
               name="West view"
               restorable="true">
         </view>
         <view
               allowMultiple="true"
               category="@@BUNDLE_SWT_NAME@@.viewCategory"
               class="@@BUNDLE_SWT_NAME@@.views.NorthView"
               icon="resources/icons/eclipse_scout.gif"
               id="@@BUNDLE_SWT_NAME@@.views.NorthView"
               name="North view"
               restorable="true">
         </view>
         <view
               allowMultiple="true"
               category="@@BUNDLE_SWT_NAME@@.viewCategory"
               class="@@BUNDLE_SWT_NAME@@.views.NorthWestView"
               icon="resources/icons/eclipse_scout.gif"
               id="@@BUNDLE_SWT_NAME@@.views.NorthWestView"
               name="North west view"
               restorable="true">
         </view>
         <view
               allowMultiple="true"
               category="@@BUNDLE_SWT_NAME@@.viewCategory"
               class="@@BUNDLE_SWT_NAME@@.views.NorthEastView"
               icon="resources/icons/eclipse_scout.gif"
               id="@@BUNDLE_SWT_NAME@@.views.NorthEastView"
               name="North east view"
               restorable="true">
         </view>
         <view
               allowMultiple="true"
               category="@@BUNDLE_SWT_NAME@@.viewCategory"
               class="@@BUNDLE_SWT_NAME@@.views.EastView"
               icon="resources/icons/eclipse_scout.gif"
               id="@@BUNDLE_SWT_NAME@@.views.EastView"
               name="East view"
               restorable="true">
         </view>
         <view
               allowMultiple="true"
               category="@@BUNDLE_SWT_NAME@@.viewCategory"
               class="@@BUNDLE_SWT_NAME@@.views.SouthView"
               icon="resources/icons/eclipse_scout.gif"
               id="@@BUNDLE_SWT_NAME@@.views.SouthView"
               name="South view"
               restorable="true">
         </view>
         <view
               allowMultiple="true"
               category="@@BUNDLE_SWT_NAME@@.viewCategory"
               class="@@BUNDLE_SWT_NAME@@.views.SouthWestView"
               icon="resources/icons/eclipse_scout.gif"
               id="@@BUNDLE_SWT_NAME@@.views.SouthWestView"
               name="South west view"
               restorable="true">
         </view>
         <view
               allowMultiple="true"
               category="@@BUNDLE_SWT_NAME@@.viewCategory"
               class="@@BUNDLE_SWT_NAME@@.views.SouthEastView"
               icon="resources/icons/eclipse_scout.gif"
               id="@@BUNDLE_SWT_NAME@@.views.SouthEastView"
               name="South east view"
               restorable="true">
         </view>
     	 <view
               allowMultiple="false"
               category="org.eclipse.ui"
               class="org.eclipse.ui.ExtensionFactory:progressView"
               icon="resources/icons/progress_view.gif"
               id="org.eclipse.ui.views.ProgressView"
               name="Progress View"
               restorable="true">
         </view>
         <category
               id="@@BUNDLE_SWT_NAME@@.viewCategory"
               name="@@ALIAS@@">
         </category>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="@@BUNDLE_SWT_NAME@@.SwtStartup">
      </startup>
   </extension>
   <extension id="application" name="Application" point="org.eclipse.core.runtime.applications">
      <application>
         <run class="@@BUNDLE_SWT_NAME@@.application.Application"/>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="@@BUNDLE_SWT_NAME@@.editor.ScoutEditorPart"
            default="false"
            icon="resources/icons/eclipse_scout.gif"
            id="@@BUNDLE_SWT_NAME@@.editor.ScoutEditorPart"
            matchingStrategy="org.eclipse.scout.rt.ui.swt.window.desktop.editor.ScoutEditorMatchingStrategy"
            name="ScoutEditorPart">
      </editor>
   </extension>
   <extension id="product" point="org.eclipse.core.runtime.products">
      <product
          application="@@BUNDLE_SWT_NAME@@.application"
          name="">
       <property
             name="appName"
             value="@@ALIAS@@"/>
       <property
          name="windowImages"
          value="resources/icons/eclipseScout16x16.gif,resources/icons/eclipseScout32x32.gif"/>
      </product>
   </extension>
   <extension
        point="org.eclipse.ui.menus">
      <menuContribution
           allPopups="false"
           locationURI="menu:org.eclipse.ui.main.menu">
        <dynamic
              class="@@BUNDLE_SWT_NAME@@.application.menu.DesktopMenuBar"
              id="org.eclipse.scout.swt.menu.dynamic">
        </dynamic>
      </menuContribution>
   </extension>
</plugin>
