<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <extension id="app" name="Application" point="org.eclipse.core.runtime.applications">
    <application>
      <run class="@@BUNDLE_SWING_NAME@@.SwingApplication"/>
    </application>
  </extension>
  <extension id="product" point="org.eclipse.core.runtime.products">
    <product
          application="@@BUNDLE_SWING_NAME@@.app"
          name="@@ALIAS@@">
       <property
             name="appName"
             value="@@ALIAS@@">
       </property>
    </product>
  </extension>
</plugin>
