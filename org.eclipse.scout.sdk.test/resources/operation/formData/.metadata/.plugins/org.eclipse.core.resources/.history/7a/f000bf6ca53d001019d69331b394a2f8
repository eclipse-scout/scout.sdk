package formdata.shared.services.process;

import org.eclipse.scout.rt.shared.data.form.fields.AbstractValueFieldData;
import org.eclipse.scout.rt.shared.data.form.AbstractFormData;

public class IgnoredFieldsFormData extends AbstractFormData {
  private static final long serialVersionUID = 1L;

  public IgnoredFieldsFormData() {
  }

  public IgnoredInteger getIgnoredInteger() {
    return getFieldByClass(IgnoredInteger.class);
  }

  public InheritedIgnored getInheritedIgnored() {
    return getFieldByClass(InheritedIgnored.class);
  }

  public NotIgnored getNotIgnored() {
    return getFieldByClass(NotIgnored.class);
  }

  public class IgnoredInteger extends AbstractValueFieldData<Integer> {
    private static final long serialVersionUID = 1L;

    public IgnoredInteger() {
    }

  }

  public class InheritedIgnored extends AbstractValueFieldData<String> {
    private static final long serialVersionUID = 1L;

    public InheritedIgnored() {
    }

  }

  public class NotIgnored extends AbstractValueFieldData<String> {
    private static final long serialVersionUID = 1L;

    public NotIgnored() {
    }

  }
}
